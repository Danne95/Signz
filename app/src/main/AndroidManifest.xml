<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hands_showprototype">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature
        android:name="android.hardware.camera2"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/signz_icon"
        android:label="@string/app_name"
        android:layoutDirection="ltr"
        android:logo="@drawable/signz_icon"
        android:roundIcon="@drawable/signz_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PhotosQueueActivity"></activity>
        <activity android:name=".UploadStatsActivity" />
        <activity android:name=".AdminStatsActivity" />
        <activity android:name=".SupportersStatsActivity" />
        <activity android:name=".SupporterQueueActivity" />
        <activity android:name=".UsersStatsActivity" />
        <activity android:name=".DeleteUserActivity" />
        <activity android:name=".SupporterActivity" />
        <activity android:name=".DemoteActivity" />
        <activity android:name=".AdminActivity" />
        <activity android:name=".ShowPicture" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LearnLanguage" />
        <activity android:name=".PicToTranslateActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>